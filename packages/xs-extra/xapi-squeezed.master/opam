opam-version: "1"
maintainer: "xen-api@lists.xen.org"
build: [
  ["./configure" "--prefix" prefix]
  [make]
  [make "install" "BINDIR=%{bin}%" "ETCDIR=%{prefix}%/etc"]
]
remove: [
    ["make" "uninstall" "BINDIR=%{bin}%" "ETCDIR=%{prefix}%/etc"]
    ["ocamlfind" "remove" "squeeze"]
]
depends: [
  "xapi-stdext"
  "ocamlfind"
  "uuidm"
  "cohttp"
  "uri"
  "re"
  "rpc"
  "xapi-idl"
  "xenstore"
  "xenstore_transport"
  "xenctrl"
]
