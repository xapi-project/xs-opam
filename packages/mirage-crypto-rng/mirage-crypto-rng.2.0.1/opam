  "dune" {>= "2.7"}
  "dune-configurator" {>= "2.0.0"}
  "duration"
  "logs"
  "mirage-crypto" {= version}
  "digestif" {>= "1.1.4"}
  "ounit2" {with-test}
  "randomconv" {with-test & >= "0.2.0"}
  "ohex" {with-test & >= "0.2.0"}
]
conflicts: [
  "mirage-runtime" {< "3.8.0"}
]
